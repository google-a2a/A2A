  "description": "A2A Types",
  "dependencies": {
    "typescript-json-schema": "^0.65.1"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "typescript": "^5.3.3"
  }
}
